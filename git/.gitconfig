[user]
    name = Matias Quintana
    email = matias.quintana.r@gmail.com
[push]
	default = simple
[core]
	editor = vim
	excludesfile = ~/.gitignore_global
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
[credential]
	helper = osxkeychain
